<?xml version='1.0' encoding='utf-8'?>
<widget id="net.ogail.najd" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ثانوية نجد</name>
    <description>
     تطبق لشؤون الموظفين والذي يظهر معلوماتهم وغياباتهم وجداولهم
    </description>
    <author email="mousaageeli@gmail.com" href="http://ogail.net">
        ogail.net
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-device" spec="1.1.5" />
    <plugin name="cordova-plugin-device-motion" spec="1.2.4" />
    <plugin name="cordova-plugin-device-orientation" spec="1.0.6" />
    <plugin name="cordova-plugin-dialogs" spec="1.3.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="1.7.0" />
    <plugin name="cordova-plugin-network-information" spec="1.3.2" />
    <plugin name="cordova-plugin-splashscreen" spec="4.0.2" />
    <plugin name="cordova-plugin-vibration" spec="2.1.4" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.2" />
    <icon src="icon.png" />
    <platform name="android">
        <icon density="ldpi" src="res/icons/Icon-36x36.png" />
        <icon density="mdpi" src="res/icons/Icon-48x48.png" />
        <icon density="hdpi" src="res/icons/Icon-72x72.png" />
        <icon density="xhdpi" src="res/icons/Icon-96x96.png" />
        <!--<icon density="xxhdpi" src="www/res/icon/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="www/res/screen/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/res/screen/android/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="www/res/screen/android/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="www/res/screen/android/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="www/res/screen/android/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="www/res/screen/android/drawable-land-xxxhdpi-screen.png" />-->
        <splash density="port-ldpi" src="res/splashscreens/splashscreen-320x426.png" />
        <splash density="port-mdpi" src="res/splashscreens/splashscreen-320x470.png" />
        <splash density="port-hdpi" src="res/splashscreens/splashscreen-480x640.png" />
        <splash density="port-xhdpi" src="res/splashscreens/android-720x960.png" />
        <!--<splash density="port-xxhdpi" src="www/res/screen/android/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="www/res/screen/android/drawable-port-xxxhdpi-screen.png" />-->
    </platform>
    <platform name="ios">
        <icon height="57" platform="ios" src="res/icons/57.png" width="57" />
        <icon height="114" platform="ios" src="res/icons/114.png" width="114" />
        <icon height="40" platform="ios" src="res/icons/40.png" width="40" />
        <icon height="80" platform="ios" src="res/icons/80.png" width="80" />
        <icon height="50" platform="ios" src="res/icons/50.png" width="50" />
        <icon height="100" platform="ios" src="res/icons/100.png" width="100" />
        <icon height="60" platform="ios" src="res/icons/60.png" width="60" />
        <icon height="120" platform="ios" src="res/icons/120.png" width="120" />
        <icon height="180" platform="ios" src="res/icons/180.png" width="180" />
        <icon height="72" platform="ios" src="res/icons/72.png" width="72" />
        <icon height="144" platform="ios" src="res/icons/144.png" width="144" />
        <icon height="76" platform="ios" src="res/icons/76.png" width="76" />
        <icon height="152" platform="ios" src="res/icons/152.png" width="152" />
        <icon height="29" platform="ios" src="res/icons/29" width="29" />
        <icon height="58" platform="ios" src="res/icons/58" width="58" />
        <icon height="87" platform="ios" src="res/icons/87" width="87" />
        <splash height="1136" platform="ios" src="res/splashscreens/splashscreen-640x1136.png" width="640" />
        <splash height="1334" platform="ios" src="res/splashscreens/750-1334.png" width="750" />
        <splash height="2208" platform="ios" src="res/splashscreens/1242-2208.png" width="1242" />
        <!--<splash height="1242" platform="ios" src="res/screen/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" platform="ios" src="res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="768" platform="ios" src="res/screen/ios/Default-Landscape~ipad.png" width="1024" />-->
        <splash height="2048" platform="ios" src="res/splashscreens/1536-2048.png" width="1536" />
        <splash height="1024" platform="ios" src="res/splashscreens/768-1024.png" width="768" />
        <splash height="960" platform="ios" src="res/splashscreens/splashscreen-640x960.png" width="640" />
        <splash height="480" platform="ios" src="res/splashscreens/splashscreen-320x480.png" width="320" />
    </platform>

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
